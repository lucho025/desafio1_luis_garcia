cription={item.description}\n             stock={item.stock}\n             initial={item.initial}\n             />\n             :\n             <h2>Cargando</h2>}\n        </div>\n    )\n}\nexport default ItemDetailContainer","/Users/Lucho/Desktop/ReactJs/desafio1_luis_garcia/src/components/ItemDetail.js",[],"/Users/Lucho/Desktop/ReactJs/desafio1_luis_garcia/src/components/CartContext.js",["89","90","91","92"],"/Users/Lucho/Desktop/ReactJs/desafio1_luis_garcia/src/components/Cart.jsx",[],"/Users/Lucho/Desktop/ReactJs/desafio1_luis_garcia/src/components/CartItem.js",[],"/Users/Lucho/Desktop/ReactJs/desafio1_luis_garcia/src/components/firebaseConfig.js",[],{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","replacedBy":"96"},{"ruleId":"97","severity":1,"message":"98","line":90,"column":6,"nodeType":"99","endLine":90,"endColumn":8,"suggestions":"100"},{"ruleId":"101","severity":1,"message":"102","line":13,"column":76,"nodeType":"103","messageId":"104","endLine":13,"endColumn":78},{"ruleId":"105","severity":1,"message":"106","line":27,"column":17,"nodeType":"107","endLine":27,"endColumn":45},{"ruleId":"105","severity":1,"message":"106","line":28,"column":17,"nodeType":"107","endLine":28,"endColumn":45},{"ruleId":"105","severity":1,"message":"106","line":29,"column":17,"nodeType":"107","endLine":29,"endColumn":45},{"ruleId":"108","severity":1,"message":"109","line":9,"column":13,"nodeType":"110","messageId":"111","endLine":9,"endColumn":20},{"ruleId":"112","severity":1,"message":"113","line":14,"column":12,"nodeType":"110","messageId":"114","endLine":14,"endColumn":28},{"ruleId":"93","replacedBy":"115"},{"ruleId":"95","replacedBy":"116"},{"ruleId":"101","severity":1,"message":"102","line":47,"column":59,"nodeType":"103","messageId":"104","endLine":47,"endColumn":61},{"ruleId":"108","severity":1,"message":"117","line":1,"column":27,"nodeType":"110","messageId":"111","endLine":1,"endColumn":36},{"ruleId":"108","severity":1,"message":"118","line":7,"column":20,"nodeType":"11